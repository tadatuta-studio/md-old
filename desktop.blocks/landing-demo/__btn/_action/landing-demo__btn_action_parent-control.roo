.landing-demo__btn_action_parent-control
{
    width: 157px;
    height: 32px;
}

.landing-demo__btn_action_parent-control .landing-demo__btn-view
{
    background-position: 50% 0;
    background-repeat: no-repeat;
}

@for $lang in $langs {
    .$lang .landing-demo__btn_action_parent-control .landing-demo__btn-view {
        background-image: url("/i/$lang/landing-demo__btn_action_parent-control.png");
    }
}
