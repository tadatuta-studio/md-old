.landing-demo__btn_action_toggle
{
    width: 85px;
    height: 43px;
}

.landing-demo__btn_action_toggle .landing-demo__btn-view
{
    outline: none;
    background-repeat: no-repeat;
}

@for $lang in $langs {
    .$lang .landing-demo__btn_action_toggle .landing-demo__btn-view {
        background-image: url("/i/$lang/landing-demo__btn_action_toggle.png");
    }
}

.landing-demo__panel_hidden_yes .landing-demo__btn_action_toggle .landing-demo__btn-view
{
    background-image: url(data:image/png;base64,);
}
